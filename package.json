{
  "name": "unitools",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "$npm_execpath run build && vite preview",
    "deploy": "$npm_execpath run build && wrangler deploy",
    "check": "run-p check:*",
    "check:biome": "biome ci .",
    "check:types": "tsgo --noEmit",
    "check:test": "vitest run --coverage=false",
    "check:cf-typegen": "$npm_execpath run cf-typegen --check",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "leveldown",
      "msw",
      "sharp",
      "workerd"
    ]
